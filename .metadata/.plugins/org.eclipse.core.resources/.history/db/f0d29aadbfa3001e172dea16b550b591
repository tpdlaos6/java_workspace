package Java05_231226;

public class Java05_6 {

	public static void main(String[] args) {
		String a="EL201800001,CH201800021,EN12231";
		String[] b=a.split(",");
		for(int i=0;i<b.length;i++) {
		}
		System.out.println(b[0]+" : 전자공학과");
		System.out.println(b[1]+" : 화학공학과");
		System.out.println(b[2]+" : 영어영문학과");
	}

}
