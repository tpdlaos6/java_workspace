package Java05_231226;

import java.util.ArrayList;

public class Java05_8 {

	public static void main(String[] args) {
		ArrayList<String> bName=new ArrayList<>();
		bName.add("삼국지");
		bName.add("수호지");
		bName.add("영웅문");

		ArrayList<String> author=new ArrayList<>();
		author.add("김씨");
		author.add("이씨");
		author.add("박씨");
		
		ArrayList<String> publisher=new ArrayList<>();
		publisher.add("삼국출판사");
		publisher.add("수호출판사");
		publisher.add("영웅출판사");
		
		ArrayList<Integer> price=new ArrayList<>();
		price.add(10000);
		price.add(20000);
		price.add(30000);
		
		System.out.println(price.get(0));
				
	}

}
