import java.util.HashMap;

public class HashMapTest {

	public static void main(String[] args) {
        HashMap<String, String> map = new HashMap<>();
        map.put("people", "사람");
        map.put("baseball", "야구");
        
        System.out.println(map.get("people")); // 키값을 지정해야, 밸류가 출력
	}

}
